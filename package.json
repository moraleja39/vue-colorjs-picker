{
  "name": "vue-colorjs-picker",
  "type": "module",
  "version": "1.0.2",
  "private": true,
  "packageManager": "pnpm@10.21.0",
  "scripts": {
    "dev": "run-p -l library:watch playground:dev",
    "library:watch": "pnpm --filter \"vue-colorjs-picker\" watch",
    "library:build": "pnpm --filter \"vue-colorjs-picker\" build && cp ./README.md ./library/README.md",
    "library:publish": "pnpm --filter \"vue-colorjs-picker\" publish",
    "playground:dev": "pnpm --filter \"playground\" dev",
    "playground:build": "pnpm --filter \"playground\" build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "pnpm --filter \"*\" type-check"
  },
  "devDependencies": {
    "@antfu/eslint-config": "6.2.0",
    "@release-it/conventional-changelog": "^10.0.2",
    "@tsconfig/node24": "24.0.1",
    "@types/node": "24.10.1",
    "@vitejs/plugin-vue": "6.0.1",
    "@vue/tsconfig": "0.8.1",
    "eslint": "9.39.1",
    "npm-run-all": "4.1.5",
    "release-it": "^19.0.6",
    "typescript": "5.9.3",
    "vite": "7.2.2",
    "vue": "3.5.24",
    "vue-tsc": "3.1.3"
  }
}
