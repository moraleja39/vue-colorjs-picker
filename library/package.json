{
  "name": "@moraleja39/vue-colorjs-picker",
  "type": "module",
  "version": "0.1.0",
  "description": "A Vue 3 color picker component built on colorjs.io",
  "author": "moraleja39",
  "license": "MIT",
  "homepage": "https://github.com/skhrvg/yourlib/tree/main/#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/moraleja39/vue-colorjs-picker.git"
  },
  "bugs": {
    "url": "https://github.com/moraleja39/vue-colorjs-picker/issues"
  },
  "exports": {
    ".": {
